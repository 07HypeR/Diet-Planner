{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "components/shared/Input.jsx",
    "components/shared/Button.jsx",
    "services/FirebaseConfig.jsx",
    "services/AiModel.jsx",
    "components/home/HomeHeader.jsx",
    "app/recipe-detail/[index].jsx",
    "app/preferance/index.jsx",
    "app/auth/SignUp.jsx",
    "app/auth/SignIn.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/Home.jsx",
    "app/(tabs)/Meals.jsx",
    "app/(tabs)/Profile.jsx",
    "app/(tabs)/Progress.jsx",
    "app/generate-ai-recipe/index.jsx",
    "app/index.jsx"
  ]
}
